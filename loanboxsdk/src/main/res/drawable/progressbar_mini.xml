<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item n1:id="@android:id/background"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <corners n1:radius="0.0dip" />
            <gradient n1:startColor="@color/me_bg_color" n1:endColor="@color/me_bg_color" n1:angle="270.0" n1:centerY="0.5" />
        </shape>
    </item>
    <item n2:id="@android:id/secondaryProgress"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <corners n2:radius="0.0dip" />
                <gradient n2:startColor="@color/me_bg_color" n2:endColor="@color/me_bg_color" n2:angle="270.0" n2:centerY="0.5" />
            </shape>
        </clip>
    </item>
    <item n3:id="@android:id/progress"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <corners n3:radius="0.0dip" />
                <gradient n3:startColor="@color/colorAccent" n3:endColor="@color/colorAccent" n3:angle="270.0" n3:centerY="0.5" />
            </shape>
        </clip>
    </item>
</layer-list>